{
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "editor.defaultFormatter": "biomejs.biome",
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "emmet.includeLanguages": {
    "njk": "html"
  },
  "formattingToggle.affects": ["editor.formatOnSave"],
  "[html]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  }
}
